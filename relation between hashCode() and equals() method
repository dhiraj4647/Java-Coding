class Student{
    int sno;
    String name;
    int className;
    public Student(int sno, String name, int className){
        this.sno=sno;
        this.name=name;
        this.className=className;
    }
    public boolean equals(Object obj){
        if(this == obj){
            return true;
        }
        else {
            Student s=(Student)obj;
            if(obj instanceof Student){
                return this.sno==s.sno && this.name==s.name && this.className==s.className;
            }
            else {
                return false;
            }
        }
    }
    public int hashCode(){
        return (sno+name.length()+className);
    }
}
class Address{
    
}
class HelloWorld {
    public static void main(String[] args) {
      Student obj1=new Student(1,"Ram",12);
      Student obj2=new Student(2,"Shyam",12);
      Student obj3=new Student(1,"Ram",12);
      Student obj4=obj2;
      System.out.println(obj1==obj2);//false
      System.out.println(obj1.equals(obj2));//false
      System.out.println(obj1.hashCode()==obj2.hashCode());//false

      System.out.println(obj1==obj3);//false
      System.out.println(obj1.equals(obj3));//true
      System.out.println(obj1.hashCode()==obj3.hashCode());//true

      System.out.println(obj2==obj4);//true
      System.out.println(obj2.equals(obj4));//true
      System.out.println(obj2.hashCode()==obj4.hashCode());//true

      Address add1=new Address();
      Address add2=new Address();
      System.out.println(add1.hashCode());//default hashcode generated by jvm
      System.out.println(add2.hashCode());
      
      System.out.println(obj1.hashCode());//developer defined hash code
      System.out.println(obj2.hashCode());
      
      
    }
}
