class Student{
    int sno;
    String name;
    int className;
    public Student(int sno, String name, int className){
        this.sno=sno;
        this.name=name;
        this.className=className;
    }
    public boolean equals(Object obj){
        if(this == obj){
            return true;
        }
        else {
            Student s=(Student)obj;
            if(obj instanceof Student){
                return this.sno==s.sno && this.name==s.name && this.className==s.className;
            }
            else {
                return false;
            }
        }
    }
    public int hashCode(){
        return (sno+name.length()+className);
    }
    
    public String toString(){
        return "sno:"+sno+"\n"+"Name:"+name+"\n"+"ClassName:"+className+"\n";
    }
    
    public int m1(){
        return super.hashCode();
    }
}
class Address{
    
}
class HelloWorld {
    public static void main(String[] args) {
      Student obj1=new Student(1,"Ram",12);
      Student obj2=new Student(2,"Shyam",12);
      Student obj3=new Student(1,"Ram",12);
      Student obj4=obj2;
      Address add=new Address();
      
      System.out.println(obj2);//prints the objects state
      System.out.println(obj3);//prints the objects state
      System.out.println(obj3.toString());//prints the objects state
      System.out.println(add);//Address@3b9a45b3

      
    }
}
/* 
Why to override
1)equals():
To compare object with states
2)hashCode():
To get the developer wanted hashcode as per state
3)toString():
To get the object state in String format.
*/
